{
	"buildOptions": {
		"additionalArguments": [
			"/ruleset:../CakeMail.RestClient.ruleset",
			"/additionalfile:../stylecop.json"
		],
		"compile": {
		},
		"emitEntryPoint": false,
		"platform": "anycpu",
		"xmlDoc": true
	},
	"configurations": {
		"Release": {
			"buildOptions": {
				"warningsAsErrors": false
			}
		}
	},
	"dependencies": {
		"Newtonsoft.Json": "9.0.1",
		"Pathoschild.Http.FluentClient": "3.0.0",
		"StyleCop.Analyzers": {
			"version": "1.1.0-beta003",
			"type": "build"
		}
	},
	"frameworks": {
		"netstandard1.3": {
			"buildOptions": {
				"define": [ "NETSTANDARD", "LIBLOG_PORTABLE" ]
			},
			"dependencies": {
				"NETStandard.Library": "1.6.1"
			},
			"imports": "dnxcore50"
		},
		"net452": {
			"buildOptions": {
				"define": [ "NETFULL", "LIBLOG_PORTABLE" ]
			},
			"dependencies": {
			},
			"frameworkAssemblies": {
				"System.Runtime.Serialization": "4.0.0"
			}
		}
	},
	"version": "7.2.0-*"
}
